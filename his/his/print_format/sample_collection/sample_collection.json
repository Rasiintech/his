 "css": "\n*{\n    margin: 0px !important;\n    padding: 0px !important;\n    font-size: 8px !important;\n}\n.barcode{\n    font-size: 8px  !important;\n}\n.main_div{\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    height: 100vh;\n}\n\n.main_div{\n      page-break-after: always; \n}",
 "custom_format": 1,
 "default_print_language": "en",
 "disabled": 0,
 "doc_type": "Sample Collection",
 "docstatus": 0,
 "doctype": "Print Format",
 "font_size": 14,
 "html": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <link rel=\"stylesheet\" href=\"styles.css\">\n</head>\n<body>\n\n{% if doc.lab_test %}\n  <!--<p>Found {{ doc.lab_test | length }} tests.</p>  Show number of tests -->\n\n  <!-- Collect unique departments using the 'unique' filter -->\n  {% set departments = doc.lab_test | map(attribute='lab__department') | unique %}\n\n  <!-- Print tests for each department -->\n  {% for department in departments %}\n    <div class=\"main_div\">\n        <p>{{ doc.patient_name or '' }}</p>\n        <span style=\"padding-left: 0px; text-wrap: nowrap; font-size: 8px;\"><strong>{{ doc.patient or '' }} \\ Lab {{ doc.token_no }}</strong>  </span>\n        <!--<p class=\"font-size: 6px;\">{{ department }} / {{ doc.token_no_for_cbc }}</p>-->\n        \n        <!-- Barcode image container -->\n        <div class=\"barcode\">\n            <img src=\"/files/barcode.png\" style=\"display:block; margin: 0 auto;\" alt=\"Barcode Image\" />\n            \n            <!-- Display current time below the barcode -->\n            <p style=\"font-size: 8px; text-align: center;\">\n                {% set current_time = frappe.utils.now() %}\n                {% set formatted_time = frappe.utils.format_datetime(current_time, \"yyyy-MM-dd HH:mm\") %}\n                {{ formatted_time }}\n            </p>\n        </div>\n\n        <!-- Print all tests for this department -->\n        {% for samp in doc.lab_test %}\n          {% if samp.lab__department == department %}\n           <!-- <p>{{ samp.lab_test }} - {{ samp.result or '' }}</p>  Corrected field names -->\n          {% endif %}\n        {% endfor %}\n    </div>\n  {% endfor %}\n  \n{% else %}\n  <p>No tests available.</p>\n{% endif %}\n\n</body>\n</html>\n",

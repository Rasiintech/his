 "css": "        .mx{\r\n            width: 700px;\r\n            margin: 0 auto;\r\n        }\r\n\r\n        .span{\r\n            text-transform: uppercase;\r\n            font-size: 18px;\r\n        }\r\n\r\n        table {\r\n            width: 100%; \r\n            border-collapse: collapse;\r\n        }\r\n\r\n        td {\r\n            /* border: 1px solid black; */\r\n            border-bottom: 1px solid black;\r\n            padding: 10px;\r\n            text-align: left;\r\n        }\r\n\r\n        .item {\r\n            display: flex;\r\n            justify-content: space-between; \r\n            align-items: center; \r\n        }\r\n\r\n        .label {\r\n            font-weight: bold;\r\n            margin-right: 10px; \r\n        }\r\n\r\n        .value {\r\n            color: black;\r\n            font-size: 18px;\r\n            width: 100%;\r\n        }\r\n        \r\n        @media print {\r\n            .scp{\r\n                display: flex; \r\n                flex-direction: column; \r\n                justify-content: space-between; \r\n                height: 100vh;\r\n            }\r\n        }",

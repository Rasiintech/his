 "html": "<style>\nhtml {\n    font-size: 100%;\n    box-sizing: border-box\n}\n\n*,\n*::before,\n*::after {\n    box-sizing: inherit\n}\n\nbody {\n    padding: 0;\n    margin: 0;\n    font-family: \"Times New Roman\", Times, serif;\n    font-size: .875rem;\n    font-weight: 300;\n    color: #000\n}\n\nh1,\nh2,\nh3 {\n    font-weight: 300;\n    color: #000;\n    margin-top: 0\n    font-family: \"Times New Roman\", Times, serif;\n}\n\np {\n    margin-top: 0\n}\n\na,\na:visited,\na:hover {\n    text-decoration: none\n}\n\n.marginX {\n    margin-inline: 50px\n}\n\n.header {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    -webkit-margin-before: 19px;\n    margin-block-start: 19px;\n    justify-content: center;\n    padding-bottom: 8px;\n    /*border-bottom: 1px solid #000*/\n}\n\n.header__top {\n    display: flex;\n    align-items: center;\n    gap: 9px\n}\n\n.header__logo {\n    width: 70px;\n    height: 70px;\n    -o-object-fit: contain;\n    object-fit: contain;\n    padding-bottom: 9px\n}\n\n.header__title {\n    font-weight: bold;\n    text-transform: uppercase\n}\n\n.header__midd {\n    display: flex;\n    align-items: center;\n    margin-left: 39px\n}\n\n.header__phone {\n    font-size: 1.125rem\n}\n\n.header__bottom {\n    display: flex;\n    flex-direction: column;\n    gap: 3px;\n    margin-left: 59px\n}\n\n.header__gmail {\n    font-size: 1.125rem\n}\n\n.header__country {\n    font-size: 1.125rem;\n    text-align: center\n}\n\n.banner {\n    display: flex;\n    flex-direction: column;\n    -webkit-margin-before: 10px;\n    margin-block-start: 10px;\n    gap: 8px\n}\n\n.banner__title {\n    text-align: center;\n    font-weight: bold;\n    font-size: 1.0rem;\n    text-transform: uppercase\n}\n\n.banner__top {\n    display: flex;\n    align-items: center;\n    justify-content: space-between\n}\n\n.banner__left {\n    border: 1px solid #000\n}\n\n.banner__no {\n    text-transform: uppercase;\n    font-weight: bold;\n    padding-left: 8px\n}\n\n.banner__id {\n    text-transform: uppercase;\n    border-left: 1px solid #000;\n    padding-left: 8px;\n    padding-right: 16px;\n    font-weight: bold\n}\n\n.banner__right {\n    font-weight: bold\n}\n\n.banner__date {\n    font-weight: bold\n}\n\n.banner__midd {\n    display: flex;\n    align-items: center;\n    gap: 18px\n}\n\n.banner__client {\n    font-weight: 400\n}\n\n.banner__clientname {\n    font-weight: bold;\n    text-transform: uppercase\n}\n\n.banner__bottom {\n    display: flex;\n    flex-direction: column;\n    gap: 0px\n}\n\n.banner__contact {\n    font-weight: 400\n}\n\n.banner__country {\n    font-weight: 400\n}\n\n.table {\n    border-collapse: collapse;\n    border-top: 1px solid #000;\n    border-left: 1px solid #000;\n    border-right: 1px solid #000;\n    -webkit-margin-before: 15px;\n    margin-block-start: 15px\n}\n\n.table__tb {\n    width: 100%\n}\n\n.table__tb thead tr th {\n    border-left: 1px solid #000 !important;\n    text-align: left\n}\n.table__tb thead tr th:last-child {\n    text-align: right\n}\n\n.table__tb thead tr {\n     border-bottom: 1px solid #000 !important;\n}\n\n.table__tb tbody tr td {\n    text-align: left;\n    border-bottom: 1px solid #000\n}\n\n.table__tb tbody tr td:not(:first-child) {\n    border-left: 1px solid #000\n}\n\n.table__tb tbody tr td:last-child {\n    text-align: right\n}\n\n.footer {\n    display: flex;\n    flex-direction: column;\n    -webkit-margin-before: 30px;\n    margin-block-start: 30px;\n    color: #000;\n    gap: 46px\n}\n\n.footer__note {\n    font-weight: 500\n}\n\n.footer__thanks {\n    font-weight: 500\n}\n\n.footer__bottom {\n    display: flex;\n    align-items: center;\n    justify-content: space-between\n}\n\n.footer__left {\n    display: flex;\n    flex-direction: column;\n    gap: 15px\n}\n\n.footer__authSig {\n    font-weight: bold;\n    text-transform: uppercase\n}\n\n.footer__line {\n    background: #000;\n    height: 1px;\n    width: 100%\n}\n\n\n\n\n/*# sourceMappingURL=style.css.map */\n</style>   \n    <header class=\"header marginX\">\n          {% if letter_head  -%}\n    <div class=\"letter-head text-center\">{{ letter_head }}</div>\n    {% endif %}\n\n    </header>\n    \n    \n\n    <section class=\"banner marginX\" style=\"margin-top: -35px\">\n        \t\t<div style=\"width: 100%\" class=\"bill_total_wrap\">\n\t\t\t\t<div class=\"bill_sec\" style=\"width: 100%\">\n\t\t\t\t    \t<div class=\"title_wrap\">\n\t\t\t\t\t\t<p class=\"title \"></p>\n\t\t\t\t\t\t<!-- <p class=\"sub_title\">Privite Limited</p> -->\n\t\t\t\t\t</div>\n\t\t\t\t\t<div style=\"border: 1px solid black; padding: 0 15px; width: 100%\">\n\t\t\t\t\t<div style=\"display: flex; align-items: center; justify-content: space-between;\">\n    \t\t\t\t\t <p style=\"flex: 1\" class=\"bold\">Patient: {{doc.patient}}</p> \n    \t\t\t\t\t<p style=\"flex: 1\" class=\"bold\">Patient Name: {{doc.patient_name}}</p> \n\t\t\t\t\t</div>\n\n                    <div style=\"display: flex; align-items: center; justify-content: space-between;\">\n                    <p style=\"flex: 1\" class=\"bold\"> {{ _(\"Age: \") }}   {{ frappe.db.get_value(\"Patient\" ,doc.patient , \"p_age\") }} {{ frappe.db.get_value(\"Patient\" ,doc.patient , \"age_type\") }} </p> \n                   <p style=\"flex: 1\" class=\"bold\">{{ _(\"Date: \") }} {{ doc.get_formatted(\"date\") }}</p> \n                    </div>\n                    \n                    <div style=\"display: flex; align-items: center; justify-content: space-between;\">\n                      <p style=\"flex: 1\" class=\"bold\">{{ _(\"Doctor: \") }} {{ doc.practitioner or frappe.db.get_value(\"Sales Invoice\", doc.reff_invoice , \"oustside\") or \"\"}}</p> \n                      {% if doc.ref_practitioner %}\n                    <p style=\"flex: 1\" class=\"bold\">{{ _(\"Doctor: \") }} {{ doc.ref_practitioner or '' }}</p> \n                    {% endif %}\n                    </div>\n                     {% if doc.oustside %}\n                    <p class=\"bold\">{{ _(\"Outsite : \") }} {{ doc.oustside or ''  }}</p> \n                    {% endif %}\n                    </div>\n                    \n                   \n\t\t\t\t\t<!--<p class=\"bold\">Age: {{doc.customer}}</p> -->\n\t          \t\n\t\t\t\t</div>\n\t\t\t\t<!--<div class=\"total_wrap \">-->\n\t\t\t\t<!--\t<p class=\"title_sec\">Invoice</p>-->\n\t          \t\t<!-- <p class=\"bold price\">USD: $1200</p> -->\n\t\t\t\t<!--\t<div class=\"info\">-->\n\t\t\t\t<!--\t\t<div class=\"inv_name\">-->\n\t\t\t\t<!--\t\t\t<p>Inovice:</p>-->\n\t\t\t\t<!--\t\t\t<p>{{doc.name or ''}}</p>-->\n\t\t\t\t<!--\t\t</div>-->\n\t\t\t\t<!--\t\t<div class=\"inv_date\" >-->\n\t\t\t\t<!--\t\t\t<p >Date:</p>-->\n\t\t\t\t<!--\t\t\t<p>{{frappe.format( doc.posting_date, {'fieldtype': 'Date'})}}</p>-->\n\t\t\t\t<!--\t\t</div>-->\n\t\t\t\t<!--\t\t<div >-->\n\t\t\t\t<!--\t\t\t<p>User:</p>-->\n\t\t\t\t<!--\t\t\t<p>{{frappe.db.get_value(\"User\" , doc.owner , \"full_name\")}}</p>-->\n\t\t\t\t<!--\t\t</div>-->\n\t\t\t\t<!--\t</div>-->\n\t\t\t\t<!--</div>-->\n\t\t\t</div>\n      <!--<h3 class=\"banner__title\">Formka Ogolaashaha qaliinka (Consent Surgery Form)</h3>-->\n\n   \n\n    <!--    <div class=\"banner__midd\">-->\n    <!--    <h4 class=\"banner__client\">PatientID:</h4>-->\n    <!--         <h4 class=\"banner__clientname\">{{ doc.patient}}</h4>    -->\n        \n        <!--DATE-->\n    <!--    &nbsp;  &nbsp;  &nbsp;  &nbsp; -->\n    <!--    <h4 class=\"banner__client\">Date:</h4>-->\n    <!--    {% if doc.date %}-->\n    <!--    <h4 class=\"banner__clientname\">{{doc.date}}</h4>-->\n    <!--    {% else %}-->\n    <!--    <h4 class=\"banner__clientname\">_________________</h4>-->\n        \n    <!--    {% endif %}-->\n        \n    <!--  </div>-->\n        \n    <!--    <div class=\"banner__midd\">-->\n    <!--    <h4 class=\"banner__client\">Patient:</h4>-->\n       \n    <!--    <h4 class=\"banner__clientname\">{{ doc.patient_name}}</h4>-->\n  